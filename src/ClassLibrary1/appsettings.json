{
  "CosmosDB": {
    "CosmosDBConnectionString": "your_cosmosdb_connection_string",
    "DatabaseName": "your_database_name",
    "ContainerName": "your_container_name"
  },
  "Vectorization": {
    "Enabled": true,
    "VectorizationAlgorithm": "your_vectorization_algorithm"
  },
  "Indexing": {
    "Enabled": true,
    "IndexingPolicy": {
      "Automatic": true,
      "IndexingMode": "Consistent",
      "IncludedPaths": [ "/*" ],
      "ExcludedPaths": [ "/\"_etag\"/?" ]
    }
  }
}
